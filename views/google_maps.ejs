<div style="width: 100%; height: 500px;" id="map-canvas"></div>

<script>

var GeoJson = <%- geoData -%>;

var map;

function initialize() {
  var mapOptions = {
    zoom: 13,
    center: new google.maps.LatLng(36.090519, -79.801034)
  };

  map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);

  map.data.addGeoJson({type:"Feature", geometry: GeoJson});
  map.data.setStyle({strokeColor: '#00e', strokeWidth: 1, fillColor: '#00e', clickable: false});
}

google.maps.event.addDomListener(window, 'load', initialize);

var geocoder = new google.maps.Geocoder();

var geocode = function(input, callback) {
  geocoder.geocode({address: input}, callback);
};

// geocode('1201 grayland street, greensboro, nc', function(results) { console.log(results); });
</script>
